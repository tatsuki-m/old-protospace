.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(prototype.captured_images.main.first.content.url), prototype_path(prototype)
    .caption
      %h3
        = prototype.title
      .proto-meta
        .proto-user
          = link_to prototype.user.name, user_path(prototype.user)
        .proto-posted
          = change_date_format(prototype.created_at)
